{
  "name": "sn-launcher",
  "version": "1.1.0",
  "description": "Search and navigate Servicenow menus, documentation, components with a simple command palette interface.",
  "scripts": {
    "start": "parcel watch src/chromium/manifest.json --host localhost --dist-dir dist/chromium",
    "build:chromium": "parcel build src/chromium/manifest.json --no-source-maps --dist-dir dist/chromium",
    "build:firefox": "parcel build src/firefox/manifest.json --no-source-maps --dist-dir dist/firefox",
    "clean": "rm -rf .parcel-cache && rm -rf dist",
    "new-component": "new-component"
  },
  "keywords": [
    "Browser",
    "extension"
  ],
  "author": "Cheng Wu",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.10",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.8.3",
    "@parcel/transformer-inline-string": "^2.8.3",
    "@types/chrome": "0.0.224",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "new-component": "^5.0.2",
    "parcel": "^2.8.3",
    "prettier": "^2.8.6",
    "process": "^0.11.10",
    "webextension-polyfill": "^0.10.0",
    "@types/webextension-polyfill": "^0.10.1"
  }
}
